<%- include('../includes/head.ejs') %>


<!--<div id="layoutSidenav">-->
    <%- include('../includes/navigation.ejs') %>

    <div class="c-wrapper c-fixed-components">
        <%- include('../../panel/includes/top.ejs') %>
        <main>
            <div class="container-fluid">

                <div class="container">
                    <%- include('../includes/flashMessage.ejs') %>

                    <div class="row mt-3">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-header"><i class="fa fa-align-justify"></i> <%= pageTitle %></div>
                                <div class="card-body">

                                    <% if (meetings.length > 0) { %>
                                        <% i = 1 %>
                                        <div class="grid">

                                            <table class="table table-responsive-sm table-bordered table-striped table-sm table-responsive"
                                                   id="myTable">
                                                <thead class="thead-light">
                                                <tr>
                                                    <th>row</th>
                                                    <th>startDate</th>
                                                    <th>endDate</th>
                                                    <th>updatedAt</th>
                                                    <th>createdAt</th>
                                                </tr>
                                                </thead>
                                                <tbody>

                                                <% for (let meeting of meetings) {
                                                %>
                                                    <tr>
                                                        <th scope="row"><%= i %></th>
                                                        <td><%= meeting.startDate %> </td>
                                                        <td><%= meeting.endDate %></td>
                                                        <td><%= meeting.updatedAt %></td>
                                                        <td><%= meeting.createdAt %></td>
                                                    </tr>

                                                    <% i++ %>

                                                <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    <% } else { %>
                                        <h1>No meetings Found!</h1>
                                    <% } %>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
<!--</div>-->


<%- include('../includes/end.ejs') %>


